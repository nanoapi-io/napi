{
  "version": "1.0.10",
  "workspace": [
    "./packages/cli"
  ],
  "nodeModulesDir": "auto",
  "lock": false,
  "tasks": {
    "dev": "deno task --config packages/cli/deno.json dev",
    "compile": "deno task --config packages/cli/deno.json compile",
    "compile:all": "deno task --config packages/cli/deno.json 'compile-*'",
    "test": "deno test -A --coverage"
  },
  "imports": {
    "@std/expect": "jsr:@std/expect@^1.0.16",
    "@std/testing": "jsr:@std/testing@^1.0.12",
    "tree-sitter-java": "npm:tree-sitter-java@^0.23.5"
  }
}
